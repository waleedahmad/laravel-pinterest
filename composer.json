{
    "name": "waleedahmad/pinterest-laravel",
    "description": "Laravel Package for the official Pinterest API",
    "authors": [
        {
            "name": "Waleed Ahmad",
            "email": "waleedgplus@gmail.com",
            "homepage": "https://github.com/waleedahmad",
